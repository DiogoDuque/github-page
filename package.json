{
  "name": "github-page",
  "version": "0.0.0-private",
  "author": "Ivo von Putzer Reibegg <ivo.putzer@gmail.com> (https://github.com/ivoputzer)",
  "license": "MIT",
  "description": "github-pages boilerplate single page application",
  "homepage": "https://github.com/ivoputzer/github-page#readme",
  "bugs": {
    "url": "https://github.com/ivoputzer/github-page/issues"
  },
  "keywords": [
    "github",
    "single",
    "page",
    "application",
    "boilerplate"
  ],
  "main": "index.js",
  "scripts": {
    "node": "node",
    "deploy": "env; gh-pages --branch website --dist ./dist --repo \"https://${GITHUB_TOKEN}@github.com/${GITHUB_REPOSITORY}.git\" --user \"${GITHUB_ACTOR} <${GITHUB_ACTOR}@users.noreply.github.com>\"",
    "build": "parcel build --public-url ./ index.html",
    "start": "parcel --open index.html"
  },
  "dependencies": {},
  "devDependencies": {
    "gh-pages": "^2.0.1",
    "parcel": "^1.12.3"
  },
  "standard": {
    "global": [
      "test"
    ],
    "ignore": [
      "dist/"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ivoputzer/github-page.git"
  }
}
